<div id="owner-detail"
  <div class="owner-detail"

    <p>Owner Name: <%= @owner.name %></p>

    <% for animal in @animals %>
    <p><%= "Animal Name:" %> <%= animal.name %> </p>
    <p><a href="/manager/animals/<%= animal.id %>">Show Animal</a></p>
    <% end %>

  </div>
</div>

<form method="get" action="/manager/owner/<%= @owner.id %>/edit">

  <input type="submit" value="Edit Owner"/>

</form>

<form method="post" action="/manager/owner/<%= @owner.id %>/delete">

  <input type="submit" value="Delete Owner"/>

</form>
